(defun X-XKeysymDB-sym->keysym (sym)
  "Lookup SYM in keysym db."
  (cdr (assq sym X-XKeysymDB)))

(defun X-XKeysymDB-keysym->sym (keysym)
  "Lookup KEYSIM in keysym db."
  (car (find keysym X-XKeysymDB :test #'= :key #'cdr)))

